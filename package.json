{
  "name": "ld-viewer-desktop",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "repository": {
    "url": "https://github.com/ikiselev1989/ld-viewer-desktop.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "prepare-release.sh ${nextRelease.version} ${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "ci(release): ${nextRelease.version}\n${nextRelease.notes}",
          "assets": [
            "package.json",
            "update.json",
            "src-tauri/tauri.conf.json"
          ]
        }
      ]
    ],
    "ci": false
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "semantic-release": "npx dotenv -e .env.local npx semantic-release"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@tauri-apps/cli": "^1.0.5",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.9",
    "axios": "^0.27.2",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.9",
    "semantic-release": "^19.0.5",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "svrollbar": "^0.12.0",
    "tailwindcss": "^3.1.8",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.2"
  }
}
